local function runScript(url)
    local s, err = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if not s then
        warn("Erro ao carregar script: "..err)
    end
end

runScript("https://raw.githubusercontent.com/realgengar/Brainrot/refs/heads/main/notify.lua")
runScript("https://raw.githubusercontent.com/tienkhanh1/spicy/main/Chilli.lua")
